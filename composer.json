{
    "name": "silicondigital/roblox",
    "description": "Roblox API for Laravel",
	"keywords": ["roblox", "laravel", "laravel7"],
    "license": "MIT",
    "authors": [
        {
            "name": "NicholasDev",
            "email": "nicholas@silicon.digital"
        }
    ],
    "require": {
        "php": ">7.3",
        "illuminate/support": "~5.5 || ~6.0 || ~7.0 || ~8.0 || ~9.0 || ~10.0"
    },
    "autoload": {
        "psr-0": {
            "SiliconDigital\\Roblox": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SiliconDigital\\Roblox\\RobloxServiceProvider"
            ],
            "aliases": {
                "Roblox": "SiliconDigital\\Roblox\\Facades\\Roblox"
            }
        }
    },
    "minimum-stability": "stable"
}
